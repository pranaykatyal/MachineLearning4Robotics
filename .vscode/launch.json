    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Debug (current file)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run HW1 with MLRenv",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/HW Folder/HW1.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "/home/pranaykatyal/miniconda3/envs/MLRenv/bin/python"
        },
        {
            "name": "Attach to debugpy (listen)",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            }
        }
    ]
}